<?xml version='1.0' encoding='UTF-8'?>  
    <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
     "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
      
     <hibernate-mapping>  
      <class name="com.sd.hib.User" table="loginstore">  
        <id name="id">  
         <generator class="assigned"></generator>  
        </id>  
                
        <property name="username"></property>  
        <property name="password"></property>  
        <property name="email"></property>  
        <property name="first_name"></property> 
        <property name="last_name"></property> 
        <property name="dob"></property> 
                
      </class>  
                
     </hibernate-mapping>  